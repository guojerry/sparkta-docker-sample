# resolve links - $0 may be a softlink
PRG="$0"

while [ -h "$PRG" ]; do
  ls=`ls -ld "$PRG"`
  link=`expr "$ls" : '.*-> \(.*\)$'`
  if expr "$link" : '/.*' > /dev/null; then
    PRG="$link"
  else
    PRG=`dirname "$PRG"`/"$link"
  fi
done

PRGDIR=`dirname "$PRG"`
BASEDIR=`cd "$PRGDIR/.." >/dev/null; pwd`

# Reset the REPO variable. If you need to influence this use the environment setup file.
REPO=


# OS specific support.  $var _must_ be set to either true or false.
cygwin=false;
darwin=false;
case "`uname`" in
  CYGWIN*) cygwin=true ;;
  Darwin*) darwin=true
           if [ -z "$JAVA_VERSION" ] ; then
             JAVA_VERSION="CurrentJDK"
           else
             echo "Using Java version: $JAVA_VERSION"
           fi
		   if [ -z "$JAVA_HOME" ]; then
		      if [ -x "/usr/libexec/java_home" ]; then
			      JAVA_HOME=`/usr/libexec/java_home`
			  else
			      JAVA_HOME=/System/Library/Frameworks/JavaVM.framework/Versions/${JAVA_VERSION}/Home
			  fi
           fi       
           ;;
esac

if [ -z "$JAVA_HOME" ] ; then
  if [ -r /etc/gentoo-release ] ; then
    JAVA_HOME=`java-config --jre-home`
  fi
fi

# For Cygwin, ensure paths are in UNIX format before anything is touched
if $cygwin ; then
  [ -n "$JAVA_HOME" ] && JAVA_HOME=`cygpath --unix "$JAVA_HOME"`
  [ -n "$CLASSPATH" ] && CLASSPATH=`cygpath --path --unix "$CLASSPATH"`
fi

# If a specific java binary isn't specified search for the standard 'java' binary
if [ -z "$JAVACMD" ] ; then
  if [ -n "$JAVA_HOME"  ] ; then
    if [ -x "$JAVA_HOME/jre/sh/java" ] ; then
      # IBM's JDK on AIX uses strange locations for the executables
      JAVACMD="$JAVA_HOME/jre/sh/java"
    else
      JAVACMD="$JAVA_HOME/bin/java"
    fi
  else
    JAVACMD=`which java`
  fi
fi

if [ ! -x "$JAVACMD" ] ; then
  echo "Error: JAVA_HOME is not defined correctly." 1>&2
  echo "  We cannot execute $JAVACMD" 1>&2
  exit 1
fi

if [ -z "$REPO" ]
then
  REPO="$BASEDIR"/repo
fi

CLASSPATH="$BASEDIR"/:/etc/sds/sparta:"$REPO"/com/stratio/sparta/driver/0.9.0/driver-0.9.0.jar:"$REPO"/com/stratio/sparta/plugins/0.9.0/plugins-0.9.0.jar:"$REPO"/org/apache/spark/spark-streaming-kafka_2.10/1.5.2/spark-streaming-kafka_2.10-1.5.2.jar:"$REPO"/org/apache/kafka/kafka_2.10/0.8.2.1/kafka_2.10-0.8.2.1.jar:"$REPO"/com/yammer/metrics/metrics-core/2.2.0/metrics-core-2.2.0.jar:"$REPO"/org/apache/kafka/kafka-clients/0.8.2.1/kafka-clients-0.8.2.1.jar:"$REPO"/com/101tec/zkclient/0.3/zkclient-0.3.jar:"$REPO"/org/apache/spark/spark-streaming-twitter_2.10/1.5.2/spark-streaming-twitter_2.10-1.5.2.jar:"$REPO"/org/twitter4j/twitter4j-stream/3.0.3/twitter4j-stream-3.0.3.jar:"$REPO"/org/twitter4j/twitter4j-core/3.0.3/twitter4j-core-3.0.3.jar:"$REPO"/org/apache/spark/spark-streaming-flume_2.10/1.5.2/spark-streaming-flume_2.10-1.5.2.jar:"$REPO"/org/apache/spark/spark-streaming-flume-sink_2.10/1.5.2/spark-streaming-flume-sink_2.10-1.5.2.jar:"$REPO"/org/apache/flume/flume-ng-core/1.6.0/flume-ng-core-1.6.0.jar:"$REPO"/org/apache/flume/flume-ng-configuration/1.6.0/flume-ng-configuration-1.6.0.jar:"$REPO"/commons-cli/commons-cli/1.2/commons-cli-1.2.jar:"$REPO"/org/apache/avro/avro-ipc/1.7.4/avro-ipc-1.7.4.jar:"$REPO"/org/apache/velocity/velocity/1.7/velocity-1.7.jar:"$REPO"/org/mortbay/jetty/jetty-util/6.1.26/jetty-util-6.1.26.jar:"$REPO"/org/mortbay/jetty/jetty/6.1.26/jetty-6.1.26.jar:"$REPO"/org/apache/mina/mina-core/2.0.4/mina-core-2.0.4.jar:"$REPO"/org/apache/flume/flume-ng-sdk/1.6.0/flume-ng-sdk-1.6.0.jar:"$REPO"/net/debasishg/redisclient_2.10/2.15/redisclient_2.10-2.15.jar:"$REPO"/commons-pool/commons-pool/1.6/commons-pool-1.6.jar:"$REPO"/org/slf4j/slf4j-api/1.7.7/slf4j-api-1.7.7.jar:"$REPO"/org/mongodb/casbah-core_2.10/2.8.0/casbah-core_2.10-2.8.0.jar:"$REPO"/org/mongodb/casbah-commons_2.10/2.8.0/casbah-commons_2.10-2.8.0.jar:"$REPO"/org/mongodb/mongo-java-driver/2.13.0/mongo-java-driver-2.13.0.jar:"$REPO"/org/mongodb/casbah-query_2.10/2.8.0/casbah-query_2.10-2.8.0.jar:"$